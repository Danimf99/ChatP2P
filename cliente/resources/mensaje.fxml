        <Label layoutX="21.0" layoutY="21.0" text="Mensajes">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>
        <Button fx:id="btnEnviar" layoutX="557.0" layoutY="379.0" mnemonicParsing="false" prefHeight="44.0" prefWidth="87.0" text="Enviar" />
        <TextArea fx:id="texto" focusTraversable="false" layoutX="14.0" layoutY="379.0" prefHeight="44.0" prefWidth="543.0" promptText="Send your message...." />
        <ScrollPane layoutX="14.0" layoutY="48.0" prefHeight="327.0" prefWidth="630.0">
            <content>
                <VBox fx:id="mensajes" prefHeight="327.0" prefWidth="630.0" />
            </content>
        </ScrollPane>
      <Button fx:id="btnEliminar" layoutX="524.0" layoutY="14.0"
              mnemonicParsing="false" text="Eliminar amigo" />
    </children>
</AnchorPane>
